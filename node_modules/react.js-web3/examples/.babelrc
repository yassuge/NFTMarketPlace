{
  "presets": [
    "react",
    "es2015",
    "stage-0",
    [
      "env",
      {
        "debug": true,
        "targets": {
          "node": "current",
          "browsers": [
            "last 2 Chrome versions"
          ]
        },
        "test": {
          "plugins": ["transform-es2015-modules-commonjs"]
        }
      }
    ]
  ],
  "plugins": [
    "transform-decorators-legacy",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}